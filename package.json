{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "microsoft-todo",
  "title": "Microsoft Todo",
  "description": "Manange tasks in Microsoft Todo from Raycast",
  "icon": "command-icon.png",
  "author": "zhangchi0104",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Microsoft Todo",
      "description": "Mnange your tasks in Microsoft Todo",
      "mode": "view"
    },
    {
      "name": "viewList",
      "title": "My Lists",
      "description": "View all lists",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@raycast/utils": "^1.5.2",
    "axios": "^1.4.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^2.29.0",
    "@raycast/eslint-config": "1.0.5",
    "@types/lodash": "^4.14.194",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
